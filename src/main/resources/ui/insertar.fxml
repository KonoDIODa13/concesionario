                  visible="false"/>
        <ComboBox fx:id="cbTipoMoto" layoutX="164.0" layoutY="349.0" prefWidth="150.0" promptText="elige una opcion"
                  visible="false"/>
        <Button layoutX="192.0" layoutY="392.0" mnemonicParsing="false" onAction="#insertar" prefHeight="31.0"
                prefWidth="56.0" text="insertar"/>
        <Button layoutX="342.0" layoutY="395.0" mnemonicParsing="false" onAction="#atras" text="Atras"/>
