        <Text layoutX="77.0" layoutY="204.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Modelo:"
              wrappingWidth="66.97265625"/>
        <Text layoutX="77.0" layoutY="242.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Precio:"
              wrappingWidth="66.97265625"/>
        <Text layoutX="77.0" layoutY="283.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Carga:"
              wrappingWidth="66.97265625"/>
        <Text layoutX="77.0" layoutY="322.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Plazas:"
              wrappingWidth="66.97265625"/>
        <Text layoutX="77.0" layoutY="366.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tipo:"
              wrappingWidth="66.97265625"/>
